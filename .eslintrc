{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "navigator": false,
    "window": false,
    "document": false,
    "registration": false,
    "fetch": false,
    "localStorage": false
  },
  "rules": {
    "jsx-a11y/anchor-is-valid": false,
    "max-len": [2, 120],
    "no-underscore-dangle": "off",
    "no-use-before-define": "off",
    "object-curly-newline": "off",
    "react/jsx-filename-extension": false
  }
}
